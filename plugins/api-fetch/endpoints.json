{
  "base_url": "https://api.flyff.com",
  "endpoints": [
    {
      "name": "data version",
      "path": "/version/data",
      "enabled": true,
      "write_pretty": true,
      "path_template": "/version"
    },
    {
      "name": "api version",
      "path": "/version/api",
      "enabled": true,
      "write_pretty": true,
      "path_template": "/version"
    },
    {
      "name": "class",
      "path": "/class",
      "enabled": true,
      "write_pretty": true,
      "bulk_ids": {
        "id_field": "id",
        "join_with": ",",
        "path_template": "/class/{id}",
        "per_id": true,
        "dest_subdir": "class_parameter"
      },
      "icons": [
        {
          "filename_field": "icon",
          "styles": ["messenger", "old_female", "old_male", "target"],
          "path_template": "/image/class/{style}/{fileName}"
        },
        {
          "filename_field": "tree",
          "styles": ["tree"],
          "path_template": "/image/class/{style}/{fileName}",
          "dest_subdir": "tree",
          "include_style_dir": false
        }
      ]
    },
    {
      "name": "monster",
      "path": "/monster",
      "enabled": true,
      "write_pretty": true,
      "bulk_ids": {
